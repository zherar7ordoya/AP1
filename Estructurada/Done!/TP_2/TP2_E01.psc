Proceso ejercicio01
	Escribir 'Ingresar dos valores, indicar e imprimir si son iguales';
	// Variables
	Definir uno,dos Como Real;
	// Inicialización
	Escribir 'Ingrese 1er valor';
	Leer uno;
	Escribir 'Ingrese 2do valor';
	Leer dos;
	// Operaciones
	Si (uno=dos) Entonces
		Escribir 'Los valores son iguales';
	FinSi
FinProceso
