// EJERCICIO 1
// Sumar 2 enteros.

Proceso sumar2enteros
	// Declaración
	Definir uno,dos,total Como Entero;
	// Inicialización
	Escribir 'Ingrese 1er entero: ';
	Leer uno;
	Escribir 'Ingrese 2do entero: ';
	Leer dos;
	// Procesamiento
	total <- uno + dos;
	// Comunicación
	Escribir 'La suma de los dos enteros es ',total;
FinProceso
